<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.OptionsController">
   <children>
      <Label layoutX="169.0" layoutY="14.0" text="Sudoku difficulties">
         <font>
            <Font name="System Bold Italic" size="30.0" />
         </font>
      </Label>
      <CheckBox fx:id="Hard" layoutX="370.0" layoutY="146.0" mnemonicParsing="false" onAction="#onlyOneCanChecked" text="Hard">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="Medium" layoutX="159.0" layoutY="146.0" mnemonicParsing="false" onAction="#onlyOneCanChecked" text="Medium">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="Easy" layoutX="372.0" layoutY="94.0" mnemonicParsing="false" onAction="#onlyOneCanChecked" text="Easy">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="Beginner" layoutX="159.0" layoutY="94.0" mnemonicParsing="false" onAction="#onlyOneCanChecked" text="Beginner">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <Button layoutX="5.0" layoutY="335.0" mnemonicParsing="false" onMouseClicked="#Back" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="251.0" layoutY="335.0" mnemonicParsing="false" onMouseClicked="#Save" text="Save">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <CheckBox fx:id="Expert" layoutX="159.0" layoutY="200.0" mnemonicParsing="false" onAction="#onlyOneCanChecked" text="Expert">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="Ultimate" layoutX="370.0" layoutY="200.0" mnemonicParsing="false" onAction="#onlyOneCanChecked" text="Ultimate">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
